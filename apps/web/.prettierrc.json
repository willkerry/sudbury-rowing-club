{
  "plugins": [
    "prettier-plugin-tailwindcss",
    "prettier-plugin-packagejson",
    "@trivago/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "^react(.*)$",
    "^next(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^@sudburyrc(.*)$",
    "^@/lib",
    "^@/components",
    "^@/",
    "^[./]",
    "^../"
  ],
  "importOrderSortSpecifiers": true
}
